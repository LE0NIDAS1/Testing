@model IEnumerable<PresentationLayer.Models.InitialGameObjectModel>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<h2>Index</h2>
<div>
    <p>
        @Html.ActionLink("Create New", "Create", "InitialGameObject", new { idGame = this.Request.QueryString["idGame"] }, null)
    </p>
</div>
<table class="table">
    <tr>
        <th hidden>
            @Html.DisplayNameFor(model => model.IdGameObject)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Quantity)
        </th>
        <th></th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td hidden>
                @Html.DisplayFor(modelItem => item.IdGameObject)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Quantity)
            </td>
            <td hidden>
                @Html.DisplayFor(modelItem => item.IdGame)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.IdGameObject, idGame = item.IdGame })
                @*@Html.ActionLink("Details", "Details", new { id = item.IdGameObject }) |*@
                @*@Html.ActionLink("Delete", "Delete", new { id = item.IdGameObject })*@
            </td>
        </tr>
    }
</table>
